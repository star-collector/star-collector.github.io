{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      ":actions": [
        "src/redux/actions"
      ],
      ":api": [
        "src/api"
      ],
      ":components": [
        "src/components"
      ],
      ":config": [
        "src/config"
      ],
      ":constants": [
        "src/constants"
      ],
      ":hooks": [
        "src/hooks"
      ],
      ":hooks/*": [
        "src/hooks/*"
      ],
      ":images/*": [
        "src/assets/images/*"
      ],
      ":pages": [
        "src/pages"
      ],
      ":pages/*": [
        "src/pages/*"
      ],
      ":store": [
        "src/redux/store"
      ],
      ":styles": [
        "src/styles"
      ],
      ":styles/*": [
        "src/styles/*"
      ],
      ":types": [
        "src/types"
      ],
      ":utils": [
        "src/utils"
      ],
      ":utils/*": [
        "src/utils/*"
      ],
      ":vectors/*": [
        "src/assets/vectors/*"
      ],
      ":widgets": [
        "src/widgets"
      ],
      ":widgets/*": [
        "src/widgets/*"
      ]
    }
  }
}
